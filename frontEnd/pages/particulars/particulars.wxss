/* pages/particulars/particulars.wxss */
.particulars{
  width:100%;
  background-color: #f8f8f8;
}

/* 头部开始 */
.particulars .title-img{
  display: flex;
  justify-content: space-between;
  position:relative;
  font-family:"MicrosoftYaHei";
  color:#000;
}
.particulars .title-img image{
  width:100%;
  height:752rpx;

}

.particulars .title-img .top{
  box-sizing:border-box;
  position:absolute;
  top:44rpx;
  left:0;
  width:100%;
  height:76rpx;
  display: flex;
  justify-content: space-between;
  padding:0 16rpx;

}
.particulars .title-img .top .diff{
   display: inline-block;
   width:76rpx;
    height:76rpx;
    opacity: 0.6;
    border-radius:50%;
    color:#fff;
    text-align: center;
    line-height: 76rpx;
    font-size:45rpx;
    background-color:#1f1f1f;
    font-weight: bold;

}
.particulars .title-img .top .part{
  display: flex;
  justify-content: space-between;
  width:178rpx;
  height:100%;

}
.particulars .title-img .top .part .moreMeg{
  position:relative;
  width:76rpx;
  height:76rpx;
}
.particulars .title-img .top .part .moreMeg .megNum{
  position: absolute;
  top:0;
  left:50%;
  transform:translateX(-50%);
  width:53rpx;
	height:38rpx;
	border-radius:20rpx;
  text-align: center;
  line-height: 38rpx;
	background-color:#fe4330;
  color:#fff;
  font-size:30rpx;
}


.particulars .title-img .bottom{
  position:absolute;
  bottom:42rpx;
  left:50%;
  display: flex;
  justify-content: space-between;
  transform:translateX(-50%);
  width:191rpx;
  height:48rpx;
}
.particulars .title-img .bottom text{
  width:91rpx;
  height:100%;
  border-radius:10rpx;	
  font-size:30rpx;
  text-align: center;
  line-height: 48rpx;
  color:#ffff;
}
.particulars .title-img .bottom .facade{
  background:#ff3000;
}
.particulars .title-img .bottom .img{
  background:rgba(255,255,255,.5);
  color:#000;
}
/* 头部结束 */

/* 信息开始 */
.particulars .megs{
  box-sizing: border-box;
  padding:19rpx 41rpx 16rpx 25rpx;
  width:100%;
  background-color: #fff;

}
.particulars .megs .meger{
  display: flex;
  justify-content: space-between;
  width:100%;
  font-size: 30rpx;
	line-height: 32rpx;
	color: "#404040";
}
.particulars .megs .meger .share{
  height:70rpx;
  font-size:20rpx;
}
.particulars .megs .meger .share text{
  display: block;
}
.particulars .megs .meger .share .icons{
  width:100%;
  text-align: center;
  font-size:40rpx;
  margin-bottom:10rpx;
 
}
.particulars .megs .meger .txt{
  width:554rpx;
  line-height:36rpx;
}
.particulars .megs .price{
  margin:25rpx 0 27rpx 0;
  font-size: 34rpx;
	color:#fa380a;
}
.particulars .allNum{
  display: flex;
  font-size: 26rpx;
	color:#505050;
}
.particulars .allNum .kuai{
  margin-right:144rpx;
}
/* 拼单开始 */
.crowdordering{
  background-color: "#f8f8f8";
  overflow: hidden;

}
.crowdordering .shop{
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  background:#fff;
  padding:23rpx 47rpx 27rpx 16rpx;
  width:100%;
  margin-top:20rpx;
    
}
