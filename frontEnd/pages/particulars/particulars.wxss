    color:#b1b1b1;

}
.crowdordering .shipping-information .inforLeft{
  align-items: center;
  flex:1;
  font-size:24rpx;
	color:#505050;
  
}
.crowdordering .shipping-information .inforLeft >text{
  flex:1;
  text-align:center;
  line-height:69rpx;
  font-weight:bold;
}
.crowdordering .shipping-information .inforLeft .circle{
  font-size:20rpx;
  color:#f00;
}
.crowdordering .shipping-information .inforLeft text text{
  display: inline-block;
  width:51rpx;
  text-align: center;
  height:69rpx;
  line-height: 69rpx;

}

.crowdordering .pinPeople{
  margin-top:10rpx;
